// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("LOCAL_DATABASE")
}

model users {
    id       Int    @id @default(autoincrement())
    name     String
    email    String @unique
    password String
}
